.radioui{display:inline-block;*display:inline;*zoom:1;vertical-align:middle;width:16px;height:16px;line-height:0;font-size:0;background:url(radioui_bg.png) no-repeat;margin-right:5px;cursor:pointer}.radioui{background-position:0 0}.radioui-checked{background-position:0 -16px}.radioui-disabled{background-position:0 -32px}.radioui-checked-disabled,.radioui-disabled-checked{background-position:0 -48px}section.board{padding:25px 20px;position:relative}.span6{width:266px}table.msg-table{width:690px}span.phone-secure-tips{border:1px solid #f93;box-shadow:#f93 0 0 10px;color:#ff7f00;padding:8px;min-width:100px;white-space:nowrap;top:8px;right:60px;background-color:#fff}span.phone-secure-tips i,span.phone-secure-tips var{width:0;height:0;position:absolute;border-width:10px;border:10px solid transparent;border-right-color:#f93;top:8px;left:-20px}span.phone-secure-tips var{border-right-color:#fff;left:-18px}input.phone{margin-right:4px}